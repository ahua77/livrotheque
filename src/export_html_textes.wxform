      'function xbDetectBrowser()\n'
      '{\n'
      '  var oldOnError = window.onerror;\n'
      '  var element = null;\n'
      '\n'
      '  window.onerror = null;\n'
      '  \n'
      '  // work around bug in xpcdom Mozilla 0.9.1\n'
      '  window.saveNavigator = window.navigator;\n'
      '\n'
      '  navigator.OS    = '#39#39';\n'
      '  navigator.version  = parseFloat(navigator.appVersion);\n'
      '  navigator.org    = '#39#39';\n'
      '  navigator.family  = '#39#39';\n'
      '\n'
      '  var platform;\n'
      '  if (typeof(window.navigator.platform) != '#39'undefined'#39')\n'
      '  {\n'
      '    platform = window.navigator.platform.toLowerCase();\n'
      '    if (platform.indexOf('#39'win'#39') != -1)\n'
      '      navigator.OS = '#39'win'#39';\n'
      '    else if (platform.indexOf('#39'mac'#39') != -1)\n'
      '      navigator.OS = '#39'mac'#39';\n'
      
        '    else if (platform.indexOf('#39'unix'#39') != -1 || platform.indexOf(' +
        #39'linux'#39') '
      '!= '
      '-1 || '
      'platform.indexOf('#39'sun'#39') != '
      '-1)\n'
      '      navigator.OS = '#39'nix'#39';\n'
      '  }\n'
      '\n'
      '  var i = 0;\n'
      '  var ua = window.navigator.userAgent.toLowerCase();\n'
      '  \n'
      '  if (ua.indexOf('#39'safari'#39') != -1)\n'
      '  {\n'
      '    i = ua.indexOf('#39'safari'#39');\n'
      '    navigator.family = '#39'safari'#39';\n'
      '    navigator.org = '#39'safari'#39';\n'
      '    navigator.version = parseFloat('#39'0'#39' + ua.substr(i+7), 10);\n'
      '  }\n'
      '  else if (ua.indexOf('#39'opera'#39') != -1)\n'
      '  {\n'
      '    i = ua.indexOf('#39'opera'#39');\n'
      '    navigator.family  = '#39'opera'#39';\n'
      '    navigator.org    = '#39'opera'#39';\n'
      '    navigator.version  = parseFloat('#39'0'#39' + ua.substr(i+6), 10);\n'
      '  }\n'
      '  else if ((i = ua.indexOf('#39'msie'#39')) != -1)\n'
      '  {\n'
      '    navigator.org    = '#39'microsoft'#39';\n'
      '    navigator.version  = parseFloat('#39'0'#39' + ua.substr(i+5), 10);\n'
      '    \n'
      '    if (navigator.version < 4)\n'
      '      navigator.family = '#39'ie3'#39';\n'
      '    else\n'
      '      navigator.family = '#39'ie4'#39'\n'
      '  }\n'
      '  else if (ua.indexOf('#39'gecko'#39') != -1)\n'
      '  {\n'
      '    navigator.family = '#39'gecko'#39';\n'
      '    var rvStart = ua.indexOf('#39'rv:'#39');\n'
      '    var rvEnd   = ua.indexOf('#39')'#39', rvStart);\n'
      '    var rv      = ua.substring(rvStart+3, rvEnd);\n'
      '    var rvParts = rv.split('#39'.'#39');\n'
      '    var rvValue = 0;\n'
      '    var exp     = 1;\n'
      '\n'
      '    for (var i = 0; i < rvParts.length; i++)\n'
      '    {\n'
      '      var val = parseInt(rvParts[i]);\n'
      '      rvValue += val / exp;\n'
      '      exp *= 100;\n'
      '    }\n'
      '    navigator.version = rvValue;\n'
      '\n'
      '    if (ua.indexOf('#39'netscape'#39') != -1)\n'
      '      navigator.org = '#39'netscape'#39';\n'
      '    else if (ua.indexOf('#39'compuserve'#39') != -1)\n'
      '      navigator.org = '#39'compuserve'#39';\n'
      '    else\n'
      '      navigator.org = '#39'mozilla'#39';\n'
      '  }\n'
      '  else if ((ua.indexOf('#39'mozilla'#39') !=-1) && '
      '(ua.indexOf('#39'spoofer'#39')==-1) && '
      '(ua.indexOf('#39'compatible'#39') '
      '== '
      '-1) && (ua.indexOf('#39'opera'#39')==-1)&& (ua.indexOf('#39'webtv'#39')==-1) && '
      '(ua.indexOf('#39'hotjava'#39')==-1))\n'
      '  {\n'
      '    var is_major = parseFloat(navigator.appVersion);\n'
      '    \n'
      '    if (is_major < 4)\n'
      '      navigator.version = is_major;\n'
      '    else\n'
      '    {\n'
      '      i = ua.lastIndexOf('#39'/'#39')\n'
      
        '      navigator.version = parseFloat('#39'0'#39' + ua.substr(i+1), 10);\' +
        'n'
      '    }\n'
      '    navigator.org = '#39'netscape'#39';\n'
      '    navigator.family = '#39'nn'#39' + parseInt(navigator.appVersion);\n'
      '  }\n'
      '  else if ((i = ua.indexOf('#39'aol'#39')) != -1 )\n'
      '  {\n'
      '    // aol\n'
      '    navigator.family  = '#39'aol'#39';\n'
      '    navigator.org    = '#39'aol'#39';\n'
      '    navigator.version  = parseFloat('#39'0'#39' + ua.substr(i+4), 10);\n'
      '  }\n'
      '  else if ((i = ua.indexOf('#39'hotjava'#39')) != -1 )\n'
      '  {\n'
      '    // hotjava\n'
      '    navigator.family  = '#39'hotjava'#39';\n'
      '    navigator.org    = '#39'sun'#39';\n'
      '    navigator.version  = parseFloat(navigator.appVersion);\n'
      '  }\n'
      '\n'
      '  window.onerror = oldOnError;\n'
      '}\n'
      '\n'
      'xbDetectBrowser();\n')
    TabOrder = 6
    Wx_BGColor = clBlack
